        with contextlib.redirect_stdout(f):
            xyz = '''H 0.0 0.0 0.0; H 0.0 0.0 %f'''%r
            mf = util.guess_from_fchk(xyz, bas, fch)
            #mf = util.guess(xyz, bas)
            mf.max_cycle = 50
            mf.kernel()
            ci = fci.FCI(mf)
            print('E(FCI) = %15.8f' % ci.kernel()[0])#INFO: ******************** input file end ********************


